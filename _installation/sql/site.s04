INSERT INTO `#__js_ticket_emailtemplates`  VALUES ('1', 'ticket-new', '', 'KiwiERP Support Ticket: New Ticket Received', '<p>Dear {USERNAME},</p>\r\n<p>Your support ticket&nbsp;<strong>{SUBJECT}</strong>&nbsp;has been submitted. We try to reply to all tickets as soon as possible, usually within 24 hours.</p>\r\n<p>Your tracking ID: {TRACKINGID}</p>\r\n<p>Your Email ID: {EMAIL}</p>\r\n<p>Ticket message: {MESSAGE}</p>\r\n<p>You can view the status of your ticket here:<br />{TICKETURL}</p>\r\n<p>You will receive an e-mail notification when our staff replies to your ticket.</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-21 11:09:00', '1'), ('2', 'department-new', NULL, 'KiwiERP Support Ticket :  New Department {DEPARTMENT_TITLE} has beed received', '<p>Hello Admin ,</p>\r\n<p>We receive new department.</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-22 07:15:12', NULL), ('3', 'group-new', NULL, 'JS Support Ticket :  New Group {GROUP_TITLE} has beed received', '<p>Hello Admin ,</p>\r\n<p>We receive new group.</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-22 07:17:17', NULL), ('4', 'staff-new', NULL, 'KiwiERP Support Ticket:  New Staff Member {STAFF_MEMBER_TITLE} has beed received', '<p>Hello Admin ,</p>\r\n<p>We receive new staff member.</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-22 07:17:26', NULL), ('5', 'helptopic-new', NULL, 'KiwiERP Support Ticket : New Help Topic {HELPTOPIC_TITLE} has beed received', '<p>Hello Admin ,</p>\r\n<p>We receive new help topic {HELPTOPIC_TITLE} of department {DEPARTMENT_TITLE}.</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-22 07:17:34', NULL), ('6', 'reassign-tk', NULL, 'KiwiERP Support Ticket: Reassign Tickets', '<p>Sucess: Ticket Reassign</p>\r\n<p>Ticket Subject : {SUBJECT}</p>\r\n<p>Your ticket has been successfully Reassign! Ticket ID:{TRACKINGID} is Reassign to Staff Member:{STAFF_MEMBER_TITLE}</p>\r\n<p>You can manage this ticket here</p>\r\n<p>{TICKETURL}</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-22 07:17:43', NULL), ('7', 'close-tk', NULL, 'KiwiERP Support Ticket:  Close Tickets', '<p>Ticket Close</p>\r\n<p>Ticket Subject : {SUBJECT}</p>\r\n<p>Ticket ID:{TRACKINGID} is Closed.</p>\r\n<p>You can manage this ticket here</p>\r\n<p>{TICKETURL}</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-22 07:17:54', NULL), ('8', 'delete-tk', NULL, 'KiwiERP Support Ticket:  Delete Ticket', '<p>Ticket Deleted</p>\r\n<p>Ticket Subject : {SUBJECT}</p>\r\n<p>Ticket ID:{TRACKINGID} is Deleted.</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-22 07:18:05', NULL), ('9', 'moverdue-tk', NULL, 'KiwiERP Support Ticket:  Ticket Mark Overdue ', '<p>Hi,</p>\r\n<p>Ticket Mark Overdue</p>\r\n<p>Ticket Subject : {SUBJECT}</p>\r\n<p>Ticket ID:{TRACKINGID} is Markoverdue.</p>\r\n<p>You can manage this ticket here</p>\r\n<p>{TICKETURL}</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-22 07:18:12', NULL), ('10', 'banemail-tk', '', 'KiwiERP Support Ticket:  Ban new email', '<p>Hi,</p>\r\n<p>Email Baned</p>\r\n<p>This email {EMAIL_ADDRESS} is Baned.</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-22 07:18:20', NULL), ('11', 'deptrans-tk', NULL, 'KiwiERP Support Ticket:  Ticket transfer to department {DEPARTMENT_TITLE}', '<p>Ticket transfer to department</p>\r\n<p>Ticket Subject : {SUBJECT}</p>\r\n<p>Ticket ID: {TRACKINGID} is transfer to department {DEPARTMENT_TITLE}.</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-22 07:18:29', NULL), ('12', 'banemailcloseticket-tk', NULL, 'KiwiERP Support Ticket: Email baned and ticket close', '<p>Email Baned and ticket close</p>\r\n<p>Ticket Subject : {SUBJECT}</p>\r\n<p>This email {EMAIL_ADDRESS} is Baned and ticket ID:{TICKETID} is closed.</p>\r\n<p><span style=\"color: red;\"><strong>*DO NOT REPLY TO THIS E-MAIL*</strong></span><br />This is an automated e-mail message sent from our support system. Do not reply to this e-mail as we wonot receive your reply!</p>', '2012-07-22 07:18:37', NU